<footer class='footer'>
  <div class='footer_nav'>
    {{#with data}}
      <div class='footer_links'>
        {{#each pages}}
          <a
            href='{{path}}'
            class='footer_link'
          >{{name}}</a>
        {{/each}}
      </div>
      <div class='copyright'>{{copyright}}</div>
    {{/with}}
  </div>
</footer>