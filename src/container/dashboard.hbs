<html>

  <head>
    <title>
      Dashboard
    </title>
    <meta charset='utf-8' />
    {{! favicon }}
    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />
    <link
      rel='preconnect'
      href='https://fonts.googleapis.com'
    />
    <link
      rel='preconnect'
      href='https://fonts.gstatic.com'
      crossorigin
    />
    <link
      href='https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,900&display=swap'
      rel='stylesheet'
    />
    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />
  </head>

  <body>
    <div class='page page--dashboard'>

      <aside class='page__aside aside vertical'>
        <div class='text aside__logo'>Sidebar</div>

        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-buttom nav-buttom--active click'
          >
            <span
              class='icon nav-buttom__icon'
              style='background-image: url("/img/svg/nav/home.svg");'
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-buttom click'>
            <span
              class='icon nav-buttom__icon'
              style='background-image: url("/img/svg/nav/order.svg");'
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-buttom click'>
            <span
              class='icon nav-buttom__icon'
              style='background-image: url("/img/svg/nav/product.svg");'
            ></span>
            <span class='text text--light'>Products</span>
          </a>

          {{! <div class='relative click'>
              <div class='field field--select'>
                <span
                  class='text text--light'
                >Choose...</span>
              </div>
              <select class='field__select'>
                <option value='dog'>Dog</option>
                <option value='cat'>Cat</option>
              </select>
            </div> }}

        </nav>
      </aside>

      <header class='header'>
        <div
          class='layout header__content text--small'
        >analytic__content
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style='background-image: url("/img/svg/header/search.svg");'
            ></span>
            <span
              class='icon icon--header-settings icon--button icon--color-blue click'
              style='background-image: url("/img/svg/header/settings.svg");'
            ></span>
          </div>
          <div class='icon__list'>
            <span
              class='icon icon--button icon--header-list icon--yellow click'
              style='background-image: url("/img/svg/header/signal.svg");'
            ></span>
            <span
              class='icon icon--button icon--header-list icon--translate-blue click'
              style='background-image: url("/img/svg/header/translate.svg");'
            ></span>
            <span
              class='icon icon--button icon--header-list icon--yellow click'
              style='background-image: url("/img/svg/header/bell.svg");'
            ></span>
          </div>
        </div>
      </header>

      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>

            <div
              class='analytic analytic--double analytic--purple'
            >

              <span
                class='icon analytic__button icon--button icon--header-list icon--money-purple click'
                style='background-image: url("/img/svg/analytic/money.svg");'
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='text text--light analytic__title'
                >$500.00</div>
                <div
                  class='text text-small text--lighter'
                >Total earning</div>
                <span
                  class='icon analytic__arrow'
                  style='background-image: url("/img/svg/analytic/purple-arrow.svg");'
                ></span>
              </div>
            </div>

            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--header-list icon--color-blue click'
                style='background-image: url("/img/svg/analytic/order.svg");'
              ></span>
              <div class='vertical vertical--small'>
                <div
                  class='text text--light analytic__title'
                >$961</div>
                <div
                  class='text text-small text--lighter'
                >Total Order</div>
              </div>
            </div>

            <div class='analytic__row analytic--blue'>
              <span
                class='icon analytic__button icon--button icon--header-list icon--color-blue click'
                style='background-image: url("/img/svg/analytic/money.svg");'
              ></span>
              <div class='vertical vertical--small'>
                <div
                  class='text text--light text--normall'
                >$203k</div>
                <div
                  class='text text-small text--lighter'
                >Total Income</div>
              </div>
            </div>
            <div
              class='analytic__row analityc__row--transporant'
            >
              <span
                class='icon analytic__button icon--button icon--header-list icon--yellow click'
                style='background-image: url("/img/svg/analytic/shop.svg");'
              ></span>
              <div class='vertical vertical--small'>
                <div
                  class='text text--heading text--normall'
                >$203k</div>
                <div class='text text-small text-gray'>Total
                  Income</div>
              </div>
            </div>
          </section>
        </div>

        <section class='checkout'>
          <div class='box purchase'>
            <div class='text text--heading'>Purchase list
            </div>
            <div class='divider'></div>
            <div class='purchase__info'></div>
          </div>

          <div class='box payment'>
            <div class='text text--heading'>Payment
            </div>
            <div class='divider'></div>
            <div class='checkbox'></div>
          </div>
        </section>
      </main>

    </div>
  </body>

</html>